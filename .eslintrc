{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/prefer-default-export": "off"
  },
  "overrides": [
    {
      "files": ["*-test.js", "*.test.jsx", "*.spec.js"],
      "rules": {
        "no-unused-expressions": "off"
      }
    }
  ]
}